textures/chex/metals/brushedmetal
{
    qer_editorimage  textures/chex/metals/brushedmetal_ed.tga       // custom DOOMEdit material preview image

    diffusemap       textures/chex/metals/brushedmetal_d.tga        // color and value information for the material
    specularmap      textures/chex/metals/brushedmetal_s.tga        // specular highlights
    bumpmap          textures/chex/metals/brushedmetal_local.tga    // normalmap for bumpmapped appearance
   {
             blend   add //gl_one, gl_dst_alpha
      map   textures/chex/metals/brushedmetal_d.tga
      rgb 0.1
   } 
}

textures/chex/metals/brushedmetal_colored
{
    qer_editorimage  textures/chex/metals/brushedmetal_ed.tga       // custom DOOMEdit material preview image

    {
        map       textures/chex/metals/brushedmetal_d.tga        // color and value information for the material
        blend diffusemap
        colored
    }
    specularmap      textures/chex/metals/brushedmetal_s.tga        // specular highlights
    bumpmap          textures/chex/metals/brushedmetal_local.tga    // normalmap for bumpmapped appearance
   {
             blend   add //gl_one, gl_dst_alpha
      map   textures/chex/metals/brushedmetal_d.tga
      red 0.1 * parm0
      blue 0.1 * parm1
      green 0.1 * parm2
   } 
}

