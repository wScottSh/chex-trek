textures/chex/floors/cementblack_mirror
{
    qer_editorimage  textures/chex/floors/cementblack_ed.tga       // custom DOOMEdit material preview image
	//discrete
	//translucent
	//noshadows
  	{
  		map        textures/chex/floors/cementblack_d.tga        // color and value information for the material
		blend diffusemap
	}
	bumpmap          textures/chex/floors/cementgrey_local.tga    // normalmap for bumpmapped appearance
  	{
		mirrorRenderMap		256 256		// size of the texture to draw the mirror view in
		blend	blend
		// tint
		red		0.7
		green	0.7
		blue	0.8
		// opacity
		alpha	0.1
		// fix up the projection
		translate	0.5 , 0.5
		scale	0.5 , 0.5	
	}
}
