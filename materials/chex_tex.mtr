textures/chex/water/pool_slow
{
    qer_editorimage models/chex/fountain/water_local
	noselfshadow
	noshadows
	//twosided
	translucent
	forceoverlays
    {
                program           heatHaze.vfp
                vertexparm              0       .1 * cosTable[ .05*time ] , .1 * sinTable[ .05*time ] // texture scrolling
                vertexparm              1       1         // magnitude of the distortion
                fragmentmap             0       _currentrender
                fragmentmap             1       models/chex/fountain/water_local   // the normal map for distortion           
    }
    {	
		maskcolor
        scroll	 .1 * cosTable[ .05*time ] , .1 * sinTable[ .05*time ]
	    map	makealpha(models/chex/fountain/water_a)
	}
	{
	    blend gl_dst_alpha, gl_one
	    maskalpha
       	cubemap		env/gen2
			red		parm0
	        green   parm1
	        blue	parm2
        texgen		reflect
	}
    {
        map         models/chex/fountain/water_local
        blend       bumpmap
    }
}