textures/chex/walls/slime
{
    qer_editorimage textures/chex/slug/slug_d       // custom DOOMEdit material preview image
    diffusemap       textures/chex/slug/slug_d        // color and value information for the material
    //specularmap      textures/chex/slug/slug_s        // specular highlights
    bumpmap          textures/chex/slug/slug_local   // normalmap for bumpmapped appearance
   {
      blend   add //gl_one, gl_dst_alpha
      map   textures/chex/slug/slug_d 
      rgb 0.1
   } 
	{
	   blend bumpmap
	   map textures/chex/slug/slug_local
	   scroll	time * .009 , time * -0.05
	}
	{
	   blend specularmap
	   map textures/chex/slug/slug_s
	   scroll	time * .1 , time * 0.05
	}
}

