//AISPLAT
textures/chex/decals/hurt
{
	qer_editorimage textures/chex/decals/hurt
	DECAL_MACRO
	translucent
	{
		map makealpha( textures/chex/decals/hurt_a2.tga )
		rotate .2 * time
		clamp
		maskcolor
	}
	{
		if ( parm7 == 0 )
		maskalpha
		blend gl_dst_alpha, gl_one
		rotate time
		map	textures/chex/decals/hurt
		clamp
	}
}
textures/chex/decals/hurt2
{
	qer_editorimage textures/chex/decals/hurt
	DECAL_MACRO
	{
		if ( parm7 == 0 )
		blend gl_src_alpha, gl_one
		rotate time
		map	textures/chex/decals/hurt
		clamp
	}
}

