//BIG SLIME ON A FLAT WALL
textures/chex/decals/bigslime
{
 	qer_editorimage textures/chex/decals/bigslime_d.tga
	noShadows
	//polygonOffset
	nonsolid
	noimpact
	surftype15
	{       
      blend	diffusemap
	 map	textures/chex/decals/bigslime_d.tga
	 alphaTest 0.5
	}
	specularmap	textures/chex/decals/bigslime_s.tga
	bumpmap 	heightmap (textures/chex/decals/bigslime_b.tga, 1)
//AMBIENT LIGHTING
   {
             blend   add //gl_one, gl_dst_alpha
      map   textures/chex/decals/bigslime_d.tga
      rgb 0.3
   } 
}

