//FLEM SPLATS
//WORLDSPLAT
textures/chex/decals/fsplat
{
	qer_editorimage	textures/chex/decals/fsplat

	nonsolid
	noimpact
	blood
	polygonoffset
	DECAL_MACRO
	surftype15
	decalInfo 9999 5 ( 1 1 1 1 ) ( 0 0 0 0 )
	{       
      	blend		gl_zero, gl_one_minus_src_color
		map			textures/chex/decals/fsplat
		clamp		// we don't want it to tile if the projection extends past the bounds
		vertexColor	// oblique projections will be slightly faded down
	}	
}
